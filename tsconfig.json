{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@icons": [
        "./src/components/icons/index.tsx"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}

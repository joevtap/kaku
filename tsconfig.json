{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@icons/*": [
        "./assets/icons/*"
      ],
      "@fonts/*": [
        "./assets/fonts/*"
      ],
      "@images/*": [
        "./assets/images/*"
      ],
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
